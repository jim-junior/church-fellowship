{
  "name": "schoolsoftapp",
  "version": "1.0.0",
  "description": "School Management System",
  "main": "index.js",
  "scripts": {
    "server": "nodemon ./src/index.ts",
    "client": "npm run start --prefix ./src/SchoolSheet",
    "client:vite": "npm run start:vite --prefix ./src/SchoolSheet",
    "client:install": "npm install --prefix ./src/SchoolSheet",
    "build:node": "tsc --build",
    "build:client": "npm run build --prefix ./src/SchoolSheet",
    "start": "ts-node ./src/index.ts",
    "build": "npm install --prefix ./src/SchoolSheet && npm run build --prefix ./src/SchoolSheet",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "electron": "electron .",
    "token": "ts-node ./src/token.ts",
    "start:forge": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish"
  },
  "author": "Robert",
  "license": "ISC",
  "dependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/express-handlebars": "^6.0.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.15.13",
    "@types/nodemailer": "^6.4.7",
    "bcrypt": "^5.1.0",
    "cloudinary": "^1.37.1",
    "concurrently": "^8.0.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "electron-squirrel-startup": "^1.0.0",
    "express": "^4.18.2",
    "express-handlebars": "^7.0.7",
    "jsonwebtoken": "^9.0.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.2.3",
    "nodemailer": "^6.9.1",
    "nodemailer-express-handlebars": "^6.1.0",
    "nodemon": "^2.0.22",
    "npm": "^9.6.5",
    "ordered-uuid": "^1.1.0",
    "react-modal": "^3.16.1",
    "reflect-metadata": "^0.1.13",
    "source-map-loader": "^4.0.1",
    "sqlite3": "^5.1.6",
    "ts-node": "^10.9.1",
    "typeorm": "^0.3.15",
    "typescript": "^5.0.4"
  },
